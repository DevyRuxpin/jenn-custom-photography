{
  "name": "jenn-custom-photography",
  "version": "1.0.0",
  "type": "module",
  "description": "Professional photo restoration & editing services - Modern Shopify photography platform with stunning 2025 UI/UX design",
  "keywords": [
    "photography",
    "photo-restoration",
    "photo-editing",
    "shopify",
    "hydrogen",
    "react",
    "typescript",
    "tailwindcss",
    "ecommerce",
    "professional-photography",
    "custom-design",
    "2025-ui-ux"
  ],
  "author": {
    "name": "Jenn Custom Photography",
    "email": "hello@jenncustomphotography.com",
    "url": "https://jenncustomphotography.com"
  },
  "license": "MIT",
  "homepage": "https://jenncustomphotography.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/jenn-custom-photography.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/jenn-custom-photography/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/assets/*.js",
    "deploy": "npm run build && npm run preview"
  },
  "dependencies": {
    "@shopify/hydrogen": "^2025.7.0",
    "@shopify/hydrogen-react": "^2025.7.0",
    "@shopify/storefront-api-client": "^1.0.9",
    "clsx": "^2.1.1",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.460.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.30.1",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "shopify",
    "photography",
    "ecommerce",
    "hydrogen",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "description": "Modern Shopify photography store with stunning UI/UX"
}
